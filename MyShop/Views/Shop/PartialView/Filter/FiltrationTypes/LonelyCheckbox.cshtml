<div class="checkbox-filter">
	<div class="input-checkbox">
		@if (Convert.ToBoolean(Model.Value.items[0].Value))
		{
			<input type="checkbox" name="@Model.Key-0" id="@Model.Key-0" value="true" checked>
		}
		else
		{
			<input type="checkbox" name="@Model.Key-0" id="@Model.Key-0" value="true">
		}
		<label for="@Model.Key-0">
			<span></span>
			@Model.Value.items[0].Meaning
		</label>
	</div>
</div>