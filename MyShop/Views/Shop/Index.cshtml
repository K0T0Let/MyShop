@{
    ViewData["Title"] = "Home page";
}

<!DOCTYPE html>
<div class="section">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="section-title">
					<h3 class="title">Новинки</h3>
				</div>
			</div>
			<div class="col-md-12">
				<div class="row">
					<div class="products-tabs">
						<div id="tab1" class="tab-pane active">
							<div class="products-slick" data-nav="#slick-nav-1">
								@await Html.PartialAsync("PartialView/Products", new List<Product>(Model.products))
							</div>
							<div id="slick-nav-1" class="products-slick-nav"></div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="section">
	<div class="container">
		<div class="row">
			@await Html.PartialAsync("PartialView/Brands", new List<Brand>(Model.brands))
		</div>
	</div>
</div>